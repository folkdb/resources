    "stringify-object": "^3.3.0"
  },
  "devDependencies": {
    "eslint": "^7.5.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.22.0",
    "graphql": "^15.3.0",
    "updtr": "^3.1.0",
    "uvu": "^0.3.3"
  },
  "engines": {
    "node": ">=14"
